{
  "name": "rohit-mandavkar-portfolio",
  "version": "1.0.0",
  "description": "Portfolio of Rohit Mandavkar - CS student, Web dev/Design & Thinker",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "analyze": "ANALYZE=true npm run build",
    "export": "next build && next export",
    "docker:build": "docker build -t portfolio .",
    "docker:run": "docker run -p 3000:3000 portfolio",
    "docker:dev": "docker-compose up --build",
    "test": "echo \"No tests specified\" && exit 0",
    "postbuild": "next-sitemap",
    "clean": "if exist .next rmdir /s /q .next && if exist out rmdir /s /q out && if exist dist rmdir /s /q dist",
    "precommit": "npm run lint:fix && npm run format",
    "prebuild": "npm run clean && npm run type-check"
  },
